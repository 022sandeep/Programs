package programs;

public class NestedForLoop {

	public static void main(String[] args)
	{

       int row,col;
       
       for(row=1;row<=3;row++)
       {
    	   
    	   for(col=3;col>=row;col--)
    	   {
    		   
    		   System.out.print("  "+col);
    	   }
    	   
    	   System.out.println("");
    	   
    	   
       }
		

	}

}
