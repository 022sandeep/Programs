package programs;

public class StringExamples {

	public static void main(String[] args) 
	{

      String str1="Madhya Pradesh";
      
      int length=str1.length();
      
      for(int i=0;i<=length-1;i++)
      {
    	  char ch=str1.charAt(i);
    	  if(ch=='a')
        	  {
        		  System.out.println(" char is:"+str1.charAt(i));
        		  
        	  } 
      }
      
      

    
	}

}
