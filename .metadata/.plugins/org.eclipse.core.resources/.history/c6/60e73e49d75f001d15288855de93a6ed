package programs;
class A
{
	int x;
}
class B extends A
{
	int y;
}
class C extends B
{
	void getxy(int p,int q)
	{
		x=p;
		y=q;
	}
	void putxy()
	{
		System.out.println("x="+x+" y="+y);
	}
}
public class MultilevelInheritanceExample2 {

	public static void main(String[] args) {
		C obj=new C();
		obj.getxy(10, 20);
		obj.putxy();

	}

}
