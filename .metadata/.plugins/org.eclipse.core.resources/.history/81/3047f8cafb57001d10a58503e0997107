package programs;

public class DoWhileExample {

	public static void main(String[] args) 
	{
		   
		int i=1;               //initalization
		do
		{
			if(i%2==0)
			{
				System.out.println("Even="+i);	
			}
			else
			{
				System.out.println("Odd="+i);
			}
			
			
			i++;
		}
		while(i<=100);
		
		
		
		
	}

}
