package programs;

public class StringExamples {

	public static void main(String[] args) 
	{

      String str1="Madhya Pradesh";
      
      int length=str1.length();
      
      while(length>0)
      {
    	  if(str1.charAt(length)=='a')
    	  {
    		  System.out.println(" char is:"+str1.charAt(length));
    		  
    	  }
    	  
    	  length--;
    	  
      }
    
	}

}
