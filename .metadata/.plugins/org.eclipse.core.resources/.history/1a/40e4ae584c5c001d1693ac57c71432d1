package programs;

class SampleClassExample1
{
	int x;
	void display()
	{
		System.out.println("x:"+x);
	}
	
	
	
}

public class SimpleClassExample 
{

	public static void main(String[] args) 
	{
		SampleClassExample1 obj=new SampleClassExample1();
             obj.x = 10;
             obj.display();

	}

}
