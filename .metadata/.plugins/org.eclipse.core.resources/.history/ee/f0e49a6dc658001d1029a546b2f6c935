package programs;

public class NestedForLoop {

	public static void main(String[] args)
	{

       int row,col;
       
       for(row=1;row<=2;row++)
       {
    	   
    	   for(col=1;col<=2;col++)
    	   {
    		   
    		   System.out.println("Row="+row+"Col="+col);
    	   }
    	   
    	   
    	   
       }
		

	}

}
