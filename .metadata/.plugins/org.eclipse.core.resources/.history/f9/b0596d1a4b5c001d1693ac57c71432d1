package programs;

class SampleClassExample
{
	int x;
	void display()
	{
		System.out.println("x:"+x);
	}
	
	
	
}




public class SimpleClassExample 
{

	public static void main(String[] args) 
	{
		SampleClassExample obj=new SampleClassExample();
             obj.x = 10;
             obj.display();

	}

}
