package programs;

public class StringExamples {

	public static void main(String[] args) 
	{

      String str1="Madhya Pradesh";
      
      // 9) substring(n,m): returns substring starting from n character to m character( not including mth character)
     
      
       String newSubstring=str1.substring(10,14);
       
       System.out.println("Substring is:"+newSubstring);
      
      
      
      
      
    
	}

}
